{
  "name": "game",
  "version": "0.1.0",
  "description": "The best game in the world",
  "repository": {
    "type": "git",
    "url": "https://github.com/mitestojanov1990/bestgame.git"
  },
  "author": "Mite Stojanov",
  "license": "MIT",
  "scripts": {
    "start": "npm run serve",
    "exec": "ts-node src/main.ts dev",
    "start:express": "nodemon --config \"./util/nodemon.json\"/",
    "build": "npm run build-ts && npm run tslint",
    "serve": "node dist/main.js",
    "watch-node": "nodemon dist/main.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "test": "jest --forceExit --coverage --verbose",
    "watch-test": "npm run test -- --watchAll",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect dist/main.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\"",
    "client": "cd client && yarn start",
    "client-build": "cd client && yarn build",
    "dev": "concurrently --kill-others-on-fail \"yarn start\" \"yarn client\""
  },
